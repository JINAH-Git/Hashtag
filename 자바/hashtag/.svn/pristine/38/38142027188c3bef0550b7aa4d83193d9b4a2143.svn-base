@charset "EUC-KR";

.sub1_info
{
	width:800px;
}
/* 실시간 인기 해시태그 리스트들 */
.phs
{
	width:350px;
	height:400px;
	position: absolute;
	top:1100px;
	left:455px;
	border-radius: 8px;
	background-color:#FFFFFF;
	margin: 0 auto;
	padding-top: 40px;
	
}
/* 실시간 인기 해시태그 제목 */
.phs_title1
{
	position: relative;
	font-size: 20px;
	padding: 10px 30px;
	border-radius: 8px;
	bottom: 20px;
	background-color:#FFFFFF;
	text-align:center;
}
/* 업데이트 시간 */
.phs_title2
{
	text-align:center;
	margin-top: -20px;
}
.phs_list
{
	width:350px;
	height:380px;
	margin-top: 10px;
	marign-left: 20px;
}
/* 실시간 인기 해시태그 순위, 해시태그 틀 */
.phslistfrm1, .phslistfrm2, .phslistfrm3, .phslistfrm4, .phslistfrm5
{
    position: relative;
    width:350px;
    height:60px;
    border-radius: 8px;
}


/* 실시간 인기 해시태그 번호와 이름들 */
.phs_list1, .phs_list2, .phs_list3, .phs_list4, .phs_list5
{
	background-color:#F2F4FD;
	border-radius:50px;
	position:relative;
   	padding: 10px 18px;
   	vertical-align: middle;
   	color: #5863CB;
   	top:30px;
   	left:50px;
   	font-size:18px;
   	text-align: center;
   	margin: 0 auto;
}

.phs_name1, .phs_name2, .phs_name3, .phs_name4, .phs_name5
{
	width:200px;
	height:30px;
	background-color:#F2F4FD;
	border-radius:100px;
	position:relative;
	top:30px;
    left:75px;
   	padding: 15px 35px;
   	vertical-align: middle;
   	color: #5863CB;
   	text-align: center;
   	font-size:18px;
   	margin: 0 auto;
}
.phs_name1 a, .phs_name2 a, .phs_name3 a, .phs_name4 a, .phs_name5 a
{
	color: #5863CB;
	text-decoration: none;
}
.ranklink
{
	text-decoration-line:none;
	color:#5863CB
}
.wrap_body
{
	min-width:1903px;
	height:750px;
	background-color:#EDEDED;
	position:relative;
	border-top: 1.5px solid #E4E4E7;
}